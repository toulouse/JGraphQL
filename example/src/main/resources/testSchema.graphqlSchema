interface Foo {
	name: String
}

interface Bar {
	name: String
	description: String
}

type Baz : Foo, Bar {
	name: String
	description: String!
	type: String
	whatever(parameter : String = "somestuff", parameter2 : Boolean = false): String
}

scalar SomeScalar

union FooOrBazOrSomeScalar = Foo | Baz | SomeScalar